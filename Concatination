#include<iostream>
#include<string.h>
using namespace std;
class String
{
	char str[200];
	public:
		void Read()
		{
			cout<<"Enter a String: ";
			cin>>str;
		}
	void print()
	{
		cout<<"String is : "<<str<<endl;
	}
	String operator + (String x)
	{
		String t;
		strcpy(t.str,str);
		strcat(t.str,x.str);
		return t;
	}
};
int main()
{
	String S1,S2,S3;
	S1.Read();
	S2.Read();
	S3=s1+s2;
	S3.print();
	return 0;
}
